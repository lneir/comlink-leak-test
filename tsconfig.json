{
	"compilerOptions": {
		"module": "esnext",
		"moduleResolution": "node",
		"sourceMap": true,
		"target": "es5",
		"lib": ["es5", "es6", "dom", "webworker" ],
		"experimentalDecorators": true,
		"outDir": "./dist/",
		"types": [
			"node"
		]
	},
	"include": [
		"./src/**/*"
	],
	"exclude": [
		"node_modules"
	]
}
